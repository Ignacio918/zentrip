{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/viator/products/search",
      "dest": "/api/viator-tours.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}
